  "bugs": {
    "url": "https://github.com/mdartic/vuejs-toulousejs-2018/issues"
  },
  "homepage": "https://github.com/mdartic/vuejs-toulousejs-2018#readme",
  "dependencies": {
    "reveal-md": "^2.0.5",
    "surge": "^0.20.1"
  }
}
